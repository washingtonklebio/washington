# Ver chave ssh da máquina 
cat /root/.ssh/id_rsa.pub

# Listar links simbolicos
ls -l /usr/bin/python

# Alterar links simbolicos
ln -sf /usr/bin/python3.6 /usr/bin/python

# Instalar Xvfb
dnf -y install xorg-x11-server-Xvfb

# Recarregar configurações do httpd
systemctl reload httpd

# Agendamentos crontab
cat /etc/crontab
